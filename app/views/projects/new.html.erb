
<div class="picture-form">

    <%= form_for(:project, url: create_project_path, :html => {:class => 'new-painting-form'}, :method => 'post') do |f| %>
        <div class="form__element">
            <%= f.text_field :name, :placeholder => 'Ime', :class => 'form__input  form-control', autofocus: true %> 
        </div>
         <div class="form__element">
            <%= f.text_field :link, :placeholder => 'Link', :class => 'form__input  form-control', autofocus: true %> 
        </div>
         <div class="form-group ">
          <div class="contact__submit">
            <%= f.submit 'DODAJ PROJEKT', :class => 'btn btn-primary' %>
          </div>
        </div>
  <% end %>


</div>